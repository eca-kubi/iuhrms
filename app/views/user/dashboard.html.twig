<script id="reservation-request-form-template" type="text/kendo-ui-template">
    <div class="container">
        <!-- New Reservation Request Form. Responsive Bootstrap form -->
        <form data-bind="events: {validate: validate}" data-role="validator" id="reservation-request-form">
            <div class="row justify-content-center">
                <div class=" col-md-12">
                    {# <div class="form-group mb-2">
                        <label for="startDate">Accommodation Start Date</label>
                        <input type="date" class="form-control" id="startDate" data-role="datepicker" required>
                    </div>

                    <div class="form-group mb-2">
                        <label for="endDate">Accommodation End Date</label>
                        <input type="date" class="form-control" id="endDate" data-role="datepicker" required>
                    </div> #}

                    <div class="form-group mb-3">
                        <label for="semester">Semester</label>
                        <input class="form-control" id="semester" data-role="dropdownlist" name="semester"
                               data-template="semester-template"
                               data-bind="source: semesters, value: selectedSemester" data-text-field="name"
                               data-value-field="name" required  data-required-msg="Please select a semester">

                    </div>

                    <!-- Hostel dropdown. Use the kendo dropdownlist widget -->
                    <div class="form-group mb-3">
                        <label for="hostel">Hostel</label>
                        <!-- The list item is a card showing a photo of the hostel, name, and type of rooms.
              Also show a button at the top of the card indicating whether the hostel has rooms available or not. -->
                        <input class="form-control" id="hostel" data-bind="value: selectedHostel, source:hostels"
                               name="hostel"
                               data-text-field="name" data-value-field="id" data-role="dropdownlist"
                               data-template="hostel-template" required data-required-msg="Please select a hostel">

                    </div>

                    <!-- Room type dropdown -->
                    <div class="form-group mb-3">
                        <label for="roomType">Room Type</label>
                        <input class="form-control" id="roomType" data-role="dropdownlist" data-text-field="type"
                               name="room_type"
                               data-value-field="type" data-bind="source: roomTypesDataSource" required data-required-msg="Please select a room type">
                    </div>

                    <div class="form-group">
                        <button class="btn btn-primary" data-role="button" type="button" data-bind="click: submit">Submit
                        </button>
                    </div>
                </div>
            </div>
        </form>
    </div>
</script>

<script id="hostel-template" type="text/x-kendo-template">
    <div class="k-card">
        <img class="k-card-image" src="#: dashboardModel.imageURL + data.image #" alt="#: data.name #"/>
        <div class="k-card-body">
            <h5 class="k-card-title">#: data.name #</h5>
            <p class="k-card-subtitle">Rooms: #: data.rooms.join(', ') #</p>
            <p class="k-card-description">#: data.description #</p>
        </div>
        <div class="k-card-actions k-card-actions-top d-none">
            <button class="k-button k-flat k-#: data.available ? 'success' : 'error' #"
                    disabled="#: !data.available #">#: data.available ? 'Available' : 'Not Available' #
            </button>
        </div>
    </div>
</script>

<script id="semester-template" type="text/x-kendo-template">
    #: data.name # - #: data.month # #: data.year #
</script>

<script>

    var viewModel = {{ viewmodel | json_encode | raw }};

    var dashboardModel = kendo.observable({
        // hostels image root url
        imageURL: "{{ URL_ROOT }}/public/images/hostels/",
        // hostels datasource
        hostels: [
            {
                id: 1,
                name: "Hostel 1",
                image: "hostel1.jpg",
                rooms: ["Single", "Double"],
                description: "Hostel 1 offers a cozy and friendly environment with clean and well-maintained rooms.",
                available: true
            },
            {
                id: 2,
                name: "Hostel 2",
                image: "hostel2.jpg",
                rooms: ["Single", "Double", "Triple"],
                description: "Hostel 2 is known for its vibrant atmosphere and its comfortable and spacious rooms.",
                available: true
            },
            {
                id: 3,
                name: "Hostel 3",
                image: "hostel3.jpg",
                rooms: ["Single", "Double", "Triple", "Quad"],
                description: "Hostel 3 provides a quiet and relaxed environment, perfect for studying and rest.",
                available: false
            },
            {
                id: 4,
                name: "Hostel 4",
                image: "hostel4.jpg",
                rooms: ["Single", "Double"],
                description: "Hostel 4 is a modern and stylish hostel with a variety of amenities for students.",
                available: true
            },
            {
                id: 5,
                name: "Hostel 5",
                image: "hostel5.jpg",
                rooms: ["Single", "Double", "Triple"],
                description: "Hostel 5 is a traditional hostel with a warm and welcoming atmosphere.",
                available: false
            },
            {
                id: 6,
                name: "Hostel 6",
                image: "hostel6.jpg",
                rooms: ["Single", "Double", "Triple", "Quad"],
                description: "Hostel 6 offers a mix of comfort and convenience, with well-equipped rooms and a great location.",
                available: true
            },
        ],

        selectedHostel: null,

        selectedSemester: null,

        semesters: new kendo.data.DataSource({
            data: [
                {
                    name: "Fall",
                    month: "September",
                    year: 2023
                },
                {
                    name: "Spring",
                    month: "January",
                    year: 2024
                },
                {
                    name: "Summer",
                    month: "May",
                    year: 2024
                },
            ]
        }),

        latestReservations: new kendo.data.DataSource({
            data: [],
        }),

        hasReservation: function () {
            return this.get('latestReservations').data().length > 0;
        },

        roomTypes: new kendo.data.DataSource({
            data: [] // Initially, the DataSource is empty
        }),
        // Use a kendo DataSource to bind the reservation requests
        reservationRequests: new kendo.data.DataSource({
            data: viewModel.reservation_requests,
            schema: {
                model: {
                    id: 'id',
                    fields: {
                        reservation_request_date: {type: 'date'},
                        check_in_date: {type: 'date'},
                        check_out_date: {type: 'date'},
                        hostel: {
                            id: 'id',
                            fields: {
                                id: {type: 'number'},
                                name: {type: 'string'},
                                available_rooms: {type: 'number'},
                                occupied_rooms: {type: 'number'},
                                total_rooms: {type: 'number'},
                                status: {type: 'string'},
                            },
                        }
                    },
                }
            },
            // Sort the reservation requests by reservation request date in descending order
            sort: {
                field: 'reservation_request_date',
                dir: 'desc'
            },
        }),

        newReservationRequest: function (e) {
            // Prevent default action
            e.preventDefault();
            // Create Kendo ui modal window with the reservation request form template
            var modal = $('<div id="hostel-modal"></div>').kendoWindow({
                title: 'New Reservation Request',
                modal: true,
                resizable: false,
                width: 'auto',
                height: 'auto',
                maxWidth: '100%',
                maxHeight: '100%',
                content: {
                    template: kendo.template($('#reservation-request-form-template').html())
                },
                // Bind the modal window to the viewmodel
                activate: function () {
                    kendo.bind(this.element, dashboardModel);
                    // Set the min and max dates of the check in and check out date pickers
                    /* var today = new Date();
                     var threeMonthsFromNow = new Date();
                     threeMonthsFromNow.setMonth(today.getMonth() + 3);

                     var endDatePicker = $("#endDate").data("kendoDatePicker");
                     endDatePicker.setOptions({
                         min: threeMonthsFromNow,
                     });

                     $("#startDate").data('kendoDatePicker').setOptions({
                         min: today,
                         change: function () {
                             var startDate = this.value();
                             var newMinEndDate = new Date(startDate);
                             newMinEndDate.setMonth(startDate.getMonth() + 3);
                             endDatePicker.min(newMinEndDate);
                         }
                     });*/

                    //createHostelsDropdown();

                    $('#hostel').data('kendoDropDownList').setOptions({
                        height: 400,
                        change: function (e) {
                            var selectedHostel = this.dataItem(); // get the selected hostel data item

                            dashboardModel.set("selectedHostel", selectedHostel);

                            // Set the roomTypes based on the first hostel
                            dashboardModel.set("roomTypesDataSource", new kendo.data.DataSource({
                                data: selectedHostel.rooms.map(function (room) {
                                    return {type: room};
                                })
                            }));

                            var roomTypeDropdown = $("#roomType").data("kendoDropDownList");
                            // Destroy the existing roomType dropdownlist
                            roomTypeDropdown.destroy();

                            // Recreate the roomType dropdownlist with the new datasource
                            $("#roomType").kendoDropDownList({
                                dataTextField: "type",
                                dataValueField: "type",
                                dataSource: selectedHostel.rooms.map(function (room) {
                                    return {type: room};
                                })
                            });
                        }
                    })
                    // Set the roomTypes based on the first hostel
                    dashboardModel.set("roomTypesDataSource", new kendo.data.DataSource({
                        data: dashboardModel.hostels[0].rooms.map(function (room) {
                            return {type: room};
                        })
                    }));
                    this.center();
                },
                // Close the modal window when the cancel button is clicked
                close: function () {
                    this.destroy();
                }

            }).data('kendoWindow');
            // Open the modal window
            modal.center().open();
        },

        // Get dataitem from onclick event
        getDataItem: function (e) {
            // If event is triggered by a Kendo ui grid command button, get the data item from the nearest grid row
            if (e.data.hasOwnProperty('commandName')) {
                return $(e.delegateTarget).data('kendoGrid').dataItem($(e.target).closest('tr'));
            }
            return e.data;
        },

        // Validate the reservation request form
        validate: function (e) {
            // Validate form
            if (e.valid) {
                // If the form is valid, get the form data
                var data = $('#reservation-request-form').serializeArray().reduce(function (obj, item) {
                    obj[item.name] = item.value;
                    return obj;
                }, {});
                console.log(data);

                // Create a new reservation request
                var reservationRequest = {
                    id: dashboardModel.latestReservations.data().length + 1,
                    // format the date
                    date: kendo.toString(new Date(), 'yyyy-MM-dd'),
                    hostel: data.hostel,
                    room_type: data.room_type,
                    semester: data.semester,
                    status: 'Pending',
                };
                // Add to latest reservations
                dashboardModel.latestReservations.add(reservationRequest);

                // show success notification
                getNotification().show({
                    title: 'Success',
                    message: 'Reservation request successfully created',
                }, 'success');
            }
        },

        submit: function(e){
            e.preventDefault();
            // Get the form validator
            var validator = $('#reservation-request-form').data('kendoValidator');
            // validate the form. if valid, close the modal window
            if (validator.validate()) {
                // Close the modal window
                $('#hostel-modal').data('kendoWindow').close();
            }

        }
    });

    // Create Views and Layouts
    var layout = new kendo.Layout('layout-template', {model: dashboardModel, wrap: false, evalTemplate: false});
    var dashboardOverview = new kendo.View('dashboard-overview', {
        model: dashboardModel,
        wrap: false,
        evalTemplate: false
    });
    var reservationRequestsView = new kendo.View('reservation-requests', {
        model: dashboardModel,
        wrap: false,
        evalTemplate: true
    });

    // Create the router
    var router = new kendo.Router({
        init: function () {
            layout.render('#wrapper');
            // Run post render code
            postViewRender();
        },
        change: function (e) {
            // navigate to the url
            console.log(e.url);
        },
    });

    // Add the routes
    router.route('/', function () {
        // Filter the reservation requests for the 4 most recent reservation requests
        dashboardModel.reservationRequests.query({
            page: 1,
            pageSize: 4,
            sort: {
                field: 'reservation_request_date',
                dir: 'desc'
            }
        });

        // Render the dashboard overview view
        layout.showIn('#content-placeholder', dashboardOverview);
    });

    // Reservation Requests route
    router.route('/reservation-requests', function () {

        // If the reservation requests data source is empty, fetch the data
        if (!dashboardModel.reservationRequests.data().length) {
            // Bind the reservation requests data source
            dashboardModel.reservationRequests.fetch();
        }

        // If the hostels data source is empty, fetch the data
        if (!dashboardModel.hostels.data().length) {
            // Bind the hostels data source
            dashboardModel.hostels.fetch();
        }

        // Set page size to 15
        dashboardModel.reservationRequests.pageSize(15);

        // Reset the reservation requests sort order
        dashboardModel.reservationRequests.sort({
            field: 'reservation_request_date',
            dir: 'desc'
        });

        // Render the reservation requests view
        layout.showIn('#content-placeholder', reservationRequestsView);

        // Get the reservation requests grid
